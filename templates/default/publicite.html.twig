<p class="detailspub">Sponsoris√©</p>
<div class="blob p-3">
            {% if add.img == false %}
                <div class="marque">
                    {{ add.marque }}
                </div>
                <div class="produit">
                    {{ add.produit }}
                </div>
                <div class="categorie">
                    Categorie : {{ add.categorie }}
                </div>
                    <div class="imgprod text-white">
                        {{ add.text }}
                    </div>
            {% else %}
                    <img class="imgprod" src="{{ asset('img/' ~ add.img ) }}" width="150" height="150"
                         alt="{{ add.marque }}">
            {% endif %}
</div>